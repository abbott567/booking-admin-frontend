{
  "contentSecurityPolicy": {
    "directives": {
      "defaultSrc": [
        "'self'"
      ],
      "scriptSrc": [
        "'self'",
        "'unsafe-inline'"
      ],
      "imgSrc": [
        "'self'"
      ],
      "fontSrc": [
        "'self'",
        "data: blob:"
      ],
      "styleSrc": [
        "'self'",
        "'unsafe-inline'"
      ]
    },
    "reportOnly": false,
    "setAllHeaders": true,
    "disableAndroid": false
  },
  "frameguard": {
    "action": "deny"
  }
}
